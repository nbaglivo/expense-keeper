{
  "name": "ExpKeep",
  "description": "Expense Keeper",
  "slug": "expense-keeper",
  "sdkVersion": "11.0.0",
  "version": "1.0.0",
  "orientation": "portrait",
  "primaryColor": "#3A4767",
  "iconUrl": "https://s30.postimg.org/gi78rjz1t/logo.png",
  "notification": {
    "iconUrl": "https://s30.postimg.org/gi78rjz1t/logo.png",
    "color": "#000000",
    "androidMode": "default",
    "androidCollapsedTitle": "#{unread_notifications} new interactions"
  },
  "loading": {
    "iconUrl": "https://s30.postimg.org/gi78rjz1t/logo.png",
    "backgroundColor": "#FFF",
    "hideExponentText": true
  },
  "appKey": "main",
  "androidStatusBarColor": "#000000",
  "androidHideExponentNotificationInShellApp": "true",
  "packagerOpts": {
    "assetExts": ["ttf", "mp4"]
  }
}
